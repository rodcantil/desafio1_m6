<p style="color: green"><%= notice %></p>

<div class="container">
  <h1 class="fs-1 fw-bolder bg-info">Movies</h1>
  <%= link_to "New movie", new_movie_path %>
  <div id="movies">
    <div class="row">
      <% @movies.each do |movie| %>
        <%= render movie %>
      <% end %>
    </div>
  </div>
</div>
